import java.util.*;

class Solution {
    public int countDigit(int n) {
        int count = 0;
        while (n != 0) {
            n = n / 10;
            count++;
        }
        return count;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution sol = new Solution();
        int num = sc.nextInt();
        int result = sol.countDigit(num);
        System.out.println(result);
    }
}
