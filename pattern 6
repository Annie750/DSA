import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner abc = new Scanner(System.in);
        int a= 5;
        int i,j;
        for(i=1;i<a+1;i++){
            for(j=1;j<=i;j++){
                System.out.print(j);
        }
            System.out.println();
        }
    }
}
