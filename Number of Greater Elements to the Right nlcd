class Solution {
    public List<Integer> count_NGE(int[] arr, int[] indices) {
        List<Integer> ans = new ArrayList<>();

        for (int idx : indices) {
            int count = 0;
            int current = arr[idx];

            for (int i = idx + 1; i < arr.length; i++) {
                if (arr[i] > current) {
                    count++;
                }
            }
            ans.add(count);
        }
        return ans;
    }
}
