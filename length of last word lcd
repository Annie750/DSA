import java.util.*;
class Solution {
    public int lengthOfLastWord(String s) {
        int i=s.length()-1;
        int count=0;
        if(s.length()==0){
            return 0;
        }
        while(i>=0){
            if(s.charAt(i)==' '){
                if(count>0){
                return count;
            }
                i--;
            }
            else{
                count++;
                i--;
            }
        }
        return count;
    }}
class main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String haystack = sc.nextLine();

        Solution sol = new Solution();
        System.out.println(sol.lengthOfLastWord(haystack));
    }
}
