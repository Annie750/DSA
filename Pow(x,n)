class Solution {
    public double myPow(double x, int n) {
            double y=1;
            if(n==0){
                return 1;
            }
            if(n>0){
         while(n!=0){
            y*=x;
            n--;
         }}
         if(n<0){
            n=n*(-1);
            while(n!=0){
            y*=(1/x);
            n--;
                }    }
                return y;
    }}
