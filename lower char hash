import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String a=sc.nextLine();
		int[] hash=new int[26];
		for(int i=0;i<a.length();i++){
		    hash[a.charAt(i)-'a']++;
		}
		int b=sc.nextInt();
		while(b>0){
		    char c=sc.next().charAt(0);
		    int result=hash[c-'a'];
		    System.out.println(result);
		    b--;
		}
}} 
