class Solution {
    /**
     * Swaps two integers and returns them as an array.
     *
     * @param a the first integer
     * @param b the second integer
     * @return an array with the two integers swapped
     */
    public int[] swap(int a, int b) {
        int[] abc = new int[2];
        int c = a ^ b ^ a;
        int d = b ^ a ^ b;

        abc[0] = c;
        abc[1] = d;

        return abc;
    }
}
