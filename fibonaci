import java.util.*;

class Solution {
    public int fib(int n) {
        if (n == 0) return 0;
        if (n == 1) return 1;
int c=fib(n - 1) + fib(n - 2);
        return c;
    }

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        Solution sol = new Solution();
        int result = sol.fib(x);
        System.out.println(result);
    }
}

