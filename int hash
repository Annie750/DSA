import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		int[] arr=new int[a];
		for(int i=0;i<a;i++){
		    arr[i]=sc.nextInt();
		}
		int[] hash=new int[13];
		for(int i=0;i<a;i++){
		    hash[arr[i]]+=1;
		}
		int b=sc.nextInt();
		while(b>0){
		    int c=sc.nextInt();
		    int result=hash[c];
		    System.out.println(result);
		}
}}
