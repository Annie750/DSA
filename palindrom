import java.util.*;
class Solution {
    public boolean isPalindrome(int n) {
        int original = n; 
        int rev = 0;
        while (n != 0) {
            int b = n % 10;    
            rev = rev * 10 + b;
            n = n / 10;        
        }
        return rev == original;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution sol = new Solution();
        int num = sc.nextInt();
        boolean result = sol.isPalindrome(num);
        System.out.println(result);
    }
}
